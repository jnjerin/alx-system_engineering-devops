#!/bin/bash

# Initialize counter
count=0

# Use while loop to display "Best School" 10 times
while [ $count -lt 10 ]
do
    # Check if it's the 9th iteration
    if [ $count -eq 8 ]
    then
        echo "Best School"
        echo "Hi"
    else
        echo "Best School"
    fi
    
    ((count++))
done
